<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <General>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="Android application"/>
      <UseAppBundle Value="False"/>
      <ResourceType Value="res"/>
    </General>
    <BuildModes>
      <Item Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <RunParams>
      <FormatVersion Value="2"/>
    </RunParams>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <Target>
      <Filename Value="android/bin/classes/Demo3DCube"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(PandroidDir)/units;$(ProjOutDir)"/>
      <OtherUnitFiles Value="$(PandroidDir)/units;$(PandroidDir)/units/pascal"/>
    </SearchPaths>
    <BuildMacros>
      <Count Value="1"/>
      <Item1>
        <Identifier Value="PandroidPkg"/>
        <Values Count="1">
          <Item1 Value="zeljus.com.demo3dcube"/>
        </Values>
      </Item1>
    </BuildMacros>
    <CodeGeneration>
      <TargetCPU Value="jvm"/>
      <TargetOS Value="android"/>
    </CodeGeneration>
